{
  "name": "order-service",
  "version": "1.0.0",
  "description": "Order service handling orders with hexagonal architecture",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/start.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write",
    "generate:swagger": "tsoa swagger",
    "generate:routes": "tsoa routes",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "tsoa",
    "hexagonal-architecture",
    "rabbitmq"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.9",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "pg": "^8.16.3",
    "tsoa": "^5.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.9",
    "@types/amqplib": "^0.10.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
